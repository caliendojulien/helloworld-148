{% extends 'base.html.twig' %}

{% block title %}
{% endblock %}

{% block body %}
    <h1>Toutes les series</h1>
    {% for serie in series %}
        <div>
            <a href="{{ path('serie_detail', {'serie': serie.id}) }}">
                <img src="{{ asset('img/series/'~serie.image) }}" alt="{{ serie.titre }}">
            </a>
        </div>
    {% endfor %}
{% endblock %}



